{
    "version": 2,
    "builds": [
      {
        "src": "next.config.mjs",  
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/app/api/$1"  
      },
      {
        "src": "/(.*)",
        "dest": "/src/app/$1"  
      }
    ]
  }
  